
<div class='columns'>
  <div class='column is-one-third'>
    <%= render partial: 'users/profile'%>
  </div>
  <div class='column is-one-third'>
    <%= render partial: 'form' %>

    <h2 class='title'>Your feed:</h2>
    <% @posts.each do |post| %>
      <%= render partial: 'displayPost', locals: {post: post} %>
    <% end %>
  </div>
  <div class='column is-one-third'>
    <%= render partial: 'users/otherUsers'%>
  </div>
</div>
